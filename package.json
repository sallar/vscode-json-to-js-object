{
    "name": "json-to-js-object",
    "displayName": "JSON to JS Object",
    "description": "Convert a JSON Object to Javascript Object",
    "version": "0.0.1",
    "publisher": "sallar",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:jsonToJSObject.convert"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "jsonToJSObject.convert",
            "title": "Convert JSON to JavaScript Object"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
